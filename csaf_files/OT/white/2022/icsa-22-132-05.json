      {
        "category": "summary",
        "text": "Successful exploitation of these vulnerabilities may allow an authenticated user to enable escalation of privilege via local access.",
        "title": "Risk evaluation"
      },
      {
        "category": "other",
        "text": "Multiple Sectors",
        "title": "Critical infrastructure sectors"
      },
