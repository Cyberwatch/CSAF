  "vulnerabilities": [
    {
      "cve": "CVE-2022-29560",
      "cwe": {
        "id": "CWE-77",
        "name": "Improper Neutralization of Special Elements used in a Command ('Command Injection')"
      },
      "notes": [
        {
          "category": "summary",
          "text": "Affected devices do not properly validate user input, making them susceptible to command injection. An attacker with access to either the shell or the web CLI with administrator privileges could access the underlying operating system as the root user.",
