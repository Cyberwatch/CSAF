  "vulnerabilities": [
    {
      "cve": "CVE-2022-33137",
      "cwe": {
        "id": "CWE-613",
        "name": "Insufficient Session Expiration"
      },
      "notes": [
        {
          "category": "summary",
          "text": "The web session management of affected devices does not invalidate session ids in certain logout scenarios. This could allow an authenticated remote attacker to hijack other users' sessions.",
          "title": "Summary"
        }
      ],
      "product_status": {
